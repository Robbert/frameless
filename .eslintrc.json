{
  "env": {
    "browser": true,
    "amd": true,
    "shared-node-browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-trailing-spaces": ["error"],
    "strict": ["error", "global"],
    "indent": ["error", 4]
  }
}
