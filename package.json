  "scripts": {
    "test": "npm run lint",
    "lint": "npm run lint:optional-closure-compiler && npm run lint:editorconfig && npm run lint:eslint && npm run lint:package-json",
    "lint:eslint": "eslint *.js src/",
    "lint:editorconfig": "eclint check *",
    "lint:closure-compiler": "gulp lint:closure-compiler",
    "lint:optional-closure-compiler": "if which java >/dev/null 2>&1; then npm run lint:closure-compiler; else true; fi",
    "lint:package-json": "gulp lint:package-json",
    "postinstall": "ln -fs .pre-commit.sh .git/hooks/pre-commit"
