  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^2.9",
    "google-closure-compiler": ">=20160315",
    "gulp": "^3.9",
    "gulp-html": "^0.4",
    "gulp-lintspaces": "^0.4",
    "js-yaml": "^3.6",
    "lodash": "^4.11",
    "media-type": "^0.3.0",
    "mime-db": "^1.23.0",
    "nih-externs": "git://github.com/wesaynih/externs.git",
    "package-json-validator": "^0.6",
    "sanitize-filename": "^1.6"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "gulp lint:chmod lint:editorconfig lint:filename lint:json lint:package-json lint:yaml && npm run lint:eslint && npm run lint:java-dependent-detection",
    "lint:eslint": "eslint *.js src/",
    "lint:filename": "gulp lint:filename",
    "lint:java-dependent": "gulp lint:closure-compiler lint:html",
    "lint:java-dependent-detection": "if which java >/dev/null 2>&1; then npm run lint:java-dependent; else true; fi",
    "lint:package-json": "gulp lint:package-json",
    "lint:yaml": "gulp lint:yaml",
    "postinstall": "ln -fs ../../.pre-commit.sh .git/hooks/pre-commit"
  }
}
